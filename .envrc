# sage.el development environment
# Load .env file if it exists
if [ -f .env ]; then
  dotenv
fi

# Export sage settings with defaults (Ollama for local dev)
export SAGE_PROVIDER="${SAGE_PROVIDER:-ollama}"
export SAGE_MODEL="${SAGE_MODEL:-llama3.2:1b}"
export SAGE_OLLAMA_HOST="${SAGE_OLLAMA_HOST:-http://localhost:11434}"
